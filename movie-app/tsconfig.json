{
  "compilerOptions": {
    "target": "ES6",
    "module": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "types": ["node"],
    "moduleResolution": "node" // Añadir esta línea para la resolución de módulos
  },
  "include": ["src"]
}
